    "./colors": "./lib/colors/exports.js",
    "./crypto": "./lib/crypto/exports.js",
    "./invariant": "./lib/invariant/exports.js",
    "./env": "./lib/env/exports.js",
    "./fs": "./lib/fs/exports.js",
    "./function": "./lib/function/exports.js",
    "./http": "./lib/http/exports.js",
    "./math": "./lib/math/exports.js",
    "./meta": "./lib/meta/exports.js",
    "./module": "./lib/module/exports.js",
    "./object": "./lib/object/exports.js",
    "./package": "./lib/package/exports.js",
    "./stdio": "./lib/stdio/exports.js",
    "./streams": "./lib/streams/exports.js",
    "./string": "./lib/string/exports.js",
    "./sync": "./lib/sync/exports.js"
  },
  "dependencies": {
